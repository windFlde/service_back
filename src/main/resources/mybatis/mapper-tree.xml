<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jk.mapper.TreeMapper">

    <!--void removeItem(@Param("ids") ArrayList<String> ids);-->
    <delete id="removeItem" >

        delete from t_tree where id in(

        <foreach collection="ids" item="id" separator=",">
            #{id}
        </foreach>

        )
    </delete>

    <!--void addTreeItemToTreeId(Tree tree);-->
    <insert id="addTreeItemToTreeId">
	  insert into t_tree (text,pid,state) values(#{text},#{pid},#{state})
	</insert>
</mapper>